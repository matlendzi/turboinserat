{
  "name": "frontend",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "react-router build",
    "build:production": "react-router build --mode production",
    "build:development": "react-router build --mode development",
    "dev": "react-router dev",
    "dev:local": "react-router dev --mode development",
    "start": "react-router-serve ./build/server/index.js",
    "start:prod": "react-router-serve ./build/server/index.js --mode production",
    "typecheck": "react-router typegen && tsc",
    "preview": "vite preview",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@react-router/node": "^7.5.3",
    "@react-router/serve": "^7.5.3",
    "axios": "^1.9.0",
    "isbot": "^5.1.27",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router": "^7.5.3"
  },
  "devDependencies": {
    "@react-router/dev": "^7.5.3",
    "@tailwindcss/vite": "^4.1.4",
    "@types/node": "^20",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4"
  }
}
